{{!< default}}

{{#post}}
<main id="page" role="main">
  <article class="{{post_class}}" typeof="person">
    <header class="post-header">
      <h1 class="post-title">{{title}}</h1>
    </header>
    <section class="post-content styled-content">
      {{content}}
    </section>
  </article>
</main>
{{/post}}
