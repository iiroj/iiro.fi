{
  "name": "iiro.fi",
  "version": "2.2.0",
  "description": "Static blog made with Gatsby",
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "licence": "UNLICENCED",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "repository": "github:iiroj/iiro.fi",
  "scripts": {
    "prestart": "rm -fr ./.cache",
    "start": "NODE_ENV=development gatsby develop",
    "lint": "eslint --ext .js --ignore-pattern public .",
    "prebuild": "rm -fr ./.cache",
    "build": "gatsby build",
    "deploy": "s3cmd sync ./public/ s3://iiro.fi --delete-removed --guess-mime-type --no-mime-magic --no-preserve --server-side-encryption --cf-invalidate",
    "publish": "npm run build && npm run deploy"
  },
  "dependencies": {
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "3.0.1",
    "gatsby": "1.0.0-alpha14",
    "gatsby-link": "1.0.0-alpha14",
    "gatsby-plugin-manifest": "1.0.0-alpha14",
    "gatsby-plugin-offline": "1.0.0-alpha14",
    "gatsby-plugin-preact": "1.0.0-alpha14",
    "gatsby-plugin-sharp": "1.0.0-alpha14",
    "gatsby-remark-copy-linked-files": "1.0.0-alpha14",
    "gatsby-remark-prismjs": "1.0.0-alpha14",
    "gatsby-remark-responsive-image": "1.0.0-alpha14",
    "gatsby-remark-smartypants": "1.0.0-alpha14",
    "gatsby-source-filesystem": "1.0.0-alpha14",
    "gatsby-transformer-remark": "1.0.0-alpha14",
    "gatsby-transformer-sharp": "1.0.0-alpha14",
    "react-helmet": "5.0.3",
    "reset.css": "2.0.2"
  }
}
