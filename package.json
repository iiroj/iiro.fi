  "repository": "gitlab:iiroj/iiro.fi",
  "private": true,
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "scripts": {
    "clean:next": "rimraf .next",
    "clean:lambda": "rimraf lambda",
    "clean:out": "rimraf out",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prestart": "npm run clean:next",
    "start": "next",
    "prestart:lambda": "npm run clean:lambda",
    "start:lambda": "NODE_ENV=development netlify-lambda serve src/lambda",
    "prebuild": "npm run clean:next",
    "build": "next build",
    "build:lambda": "netlify-lambda build src/lambda",
    "preexport": "npm run clean:out",
    "export": "next export",
    "postexport": "cp static_netlify/* out/"
  },
  "dependencies": {
    "css-wipe": "^4.3.0",
    "dotenv": "^5.0.1",
    "polished": "^1.9.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "recompose": "^0.26.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "eslint": "^4.19.1",
