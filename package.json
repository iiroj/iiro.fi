    "netlify-lambda": "^1.0.0-babel-7-beta",
    "next": "^6.0.1",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "lambda",
    "netlify",
    "nextjs",
    "react",
    "serverless",
    "static-site",
    "styled-components"
