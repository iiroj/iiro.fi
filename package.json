    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "history": "^4.10.1",
    "html-renderer-webpack-plugin": "^5.1.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
