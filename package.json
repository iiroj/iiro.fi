{
  "name": "iiro.fi",
  "version": "22.1.5",
  "description": "A static site",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iiroj/iiro.fi.git"
  },
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "lint:base": "eslint --ext .js,.ts,.tsx",
    "lint": "npm run lint:base -- .",
    "typecheck": "tsc --noEmit",
    "version": "standard-version --no-verify",
    "check": "npm run lint && npm run typecheck && npm run build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint:base --"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run check"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-next.12",
    "next": "^9.3.1",
    "polished": "^3.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.4",
    "react-style-reset": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@emotion/babel-preset-css-prop": "^11.0.0-next.10",
    "@emotion/styled": "^11.0.0-next.11",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-polished": "^1.1.0",
    "commitlint": "^8.3.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "now": "^17.0.4"
  },
  "keywords": [
    "react",
    "serverless",
    "static-site"
  ]
}
