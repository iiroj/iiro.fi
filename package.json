    "gatsby-plugin-netlify": "^2.0.0-rc.6",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "babel-plugin-emotion": "^9.2.9",
    "concurrently": "^4.0.1",
