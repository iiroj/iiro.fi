{
  "name": "iiro.fi",
  "version": "22.2.0",
  "description": "A static site",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iiroj/iiro.fi.git"
  },
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "lint:base": "eslint --ext .js,.ts,.tsx",
    "lint": "npm run lint:base -- .",
    "typecheck": "tsc --noEmit",
    "version": "standard-version --no-verify",
    "check": "npm run lint && npm run typecheck && npm run build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint:base --"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run check"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-next.13",
    "next": "^9.4.4",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-style-reset": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@emotion/babel-preset-css-prop": "^11.0.0-next.10",
    "@emotion/styled": "^11.0.0-next.11",
    "@types/node": "^14.0.18",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/webpack": "4.41.20",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-polished": "^1.1.0",
    "commitlint": "^9.0.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.6"
  },
  "keywords": [
    "react",
    "serverless",
    "static-site"
  ]
}
