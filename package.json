{
  "name": "iiro.fi",
  "version": "22.4.0",
  "description": "A static site",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iiroj/iiro.fi.git"
  },
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "private": true,
  "scripts": {
    "start": "next",
    "build": "next build && next export -o dist",
    "lint:base": "eslint --ext .js,.ts,.tsx",
    "lint": "npm run lint:base -- .",
    "typecheck": "tsc --noEmit",
    "version": "standard-version --no-verify",
    "check": "npm run lint && npm run typecheck && npm run build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint:base --"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run check"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "next": "^10.0.4",
    "polished": "^4.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-style-reset": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/webpack": "4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-polished": "^1.1.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "react",
    "serverless",
    "static-site"
  ]
}
