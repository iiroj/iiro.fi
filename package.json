{
  "name": "@iiro.fi/blog",
  "version": "0.1.0",
  "description": "Static blog made with Gatsby",
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "licence": "UNLICENCED",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "repository": "github:iiroj/iiro.fi",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "npm run lint",
    "build": "gatsby build",
    "predeploy": "npm run build",
    "deploy": "./utils/deploy.sh"
  },
  "dependencies": {
    "gatsby": "0.12.19",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-helmet": "3.2.2"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-preset-babili": "0.0.8",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "eslint": "3.9.1",
    "eslint-config-standard": "6.2.1",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-promise": "3.3.1",
    "eslint-plugin-react": "6.6.0",
    "eslint-plugin-standard": "2.0.1",
    "toml-js": "0.0.8"
  },
  "private": true,
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-class-properties",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "__PREFIX_LINKS__": true
    }
  }
}
