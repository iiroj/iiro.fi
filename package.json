    "@types/react-dom": "19.1.1",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "esbuild": "0.25.2",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "nodemon": "3.1.9",
    "prettier": "3.5.3",
