{
  "name": "iiro.fi",
  "version": "10.8.1",
  "description": "A static site built with next.js",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "licence": "MIT",
  "repository": "github:iiroj/iiro.fi",
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "private": true,
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "scripts": {
    "clean:next": "rimraf .next",
    "clean:lambda": "rimraf build_lambda",
    "clean:app": "rimraf build_app",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prestart": "npm run clean:next",
    "start": "next",
    "prestart:lambda": "npm run clean:lambda",
    "start:lambda": "NODE_ENV=development netlify-lambda serve lambda",
    "prebuild": "npm run clean:next",
    "build": "next build",
    "build:lambda": "netlify-lambda build lambda",
    "preexport": "npm run clean:app",
    "export": "next export -o build_app"
  },
  "dependencies": {
    "css-wipe": "^4.3.0",
    "fontfaceobserver": "^2.0.13",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
