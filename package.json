{
  "name": "iiro.fi",
  "version": "24.1.1",
  "description": "A static site",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iiroj/iiro.fi.git"
  },
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "private": true,
  "type": "module",
  "workspaces": [
    "site"
  ],
  "scripts": {
    "start": "npm run -w=site start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "typecheck": "tsc --project site/tsconfig.json",
    "build": "npm run --workspaces build",
    "serverless:state:download": "aws s3 sync s3://iiro.fi-serverless-state .serverless --delete",
    "serverless:deploy": "serverless-patched --debug",
    "serverless:state:upload": "aws s3 sync .serverless s3://iiro.fi-serverless-state --delete",
    "version": "standard-version --no-verify"
  },
  "dependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@sls-next/serverless-component": "3.6.0",
    "@sls-next/serverless-patched": "3.4.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "prettier": "2.4.1",
    "standard-version": "9.3.2",
    "typescript": "4.5.2"
  }
}
