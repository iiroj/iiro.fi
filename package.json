{
  "name": "iiro.fi",
  "version": "21.6.2",
  "description": "A Static site",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": "github:iiroj/iiro.fi",
  "bugs": "https://github.com/iiroj/iiro.fi/issues",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server -r register.ts --watch-content-base --silent --port $PORT",
    "build": "NODE_ENV=production webpack -r register.ts",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.sw.json --noEmit",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "devDependencies": {
