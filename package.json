    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.4.0",
    "@types/webpack-env": "^1.14.1",
    "@types/workbox-sw": "^4.3.0",
