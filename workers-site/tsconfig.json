{
    "compilerOptions": {
        "allowJs": false,
        "lib": ["ESNext", "WebWorker"],
        "module": "CommonJS",
        "moduleResolution": "Node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "./dist",
        "removeComments": true,
        "resolveJsonModule": true,
        "rootDir": "./src",
        "strict": true,
        "target": "ESNext",
        "types": ["@cloudflare/workers-types"]
    },
    "exclude": ["node_modules"]
}
