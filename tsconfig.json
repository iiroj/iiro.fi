{
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react",
        "lib": ["DOM", "ES2020"],
        "module": "ES2020",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "strict": true,
        "target": "ES2020"
    },
    "exclude": ["dist", "node_modules", "public", "workers-site"]
}
